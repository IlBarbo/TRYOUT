<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="math.calculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Calcolatrice"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Calcolatrice">

        <activity android:name=".Convertitore"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".CronologiaCalc"
            android:parentActivityName=".Activity_calc"
            />
        <activity android:name=".CronologiaConv"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
           />
        <activity android:name=".CronologiaBase"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".ConvertitoreBase"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".Grafico"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity_calc" />
        <activity
            android:name=".Main_Activity"
            android:label="Calcolatrice"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>